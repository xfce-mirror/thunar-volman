msgid "Encrypted volume"
msgstr "暗号化ボリューム"

#: ../thunar-volman/tvm-crypto-volume.c:297
msgid "<span weight=\"bold\" size=\"larger\">Mounting encrypted volume...</span>"
msgstr "<span weight=\"bold\" size=\"larger\">暗号化ボリュームをマウントしています...</span>"

